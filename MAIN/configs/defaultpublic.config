//
// RTCWPro Default Public
//
// !!! IMPORTANT NOTE !!!
// Ensure all values are surrounded by quotes, the parser can't properly deal with negative values otherwise.

configname "^1RTCWPro ^7Public"

public

init
{
setl g_tournament "0"
setl g_altStopwatchMode "0"
setl g_complaintlimit "6"
setl g_doWarmup "0"
setl g_inactivity "0"
setl g_maxlives "0"
setl g_teamforcebalance "1"
setl g_voicechatsallowed "50"
setl g_warmup "10"
setl g_antiWarp "1"
setl g_antilag "1"
setl g_noTeamSwitching "1"
setl g_minGameClients "2"
setl match_minclients "2"
setl match_latejoin "1"
setl match_mutespecs "1"
setl match_timeoutcount "0"
setl match_warmupDamage "0"
setl team_maxplayers "0"
setl team_nocontrols "1"
setl sv_allowDownload "1"
setl sv_floodProtect "0"
setl sv_fps "20"
setl vote_limit "5"
setl g_lifeStats "1"
setl g_crouchRate "5"
setl g_fastStabSound "3"

// reset cvars which might be set (but no changed) when switching from public to comp configs
set g_userTimeLimit "0"
set g_useralliedrespawntime "0"
set g_useraxisrespawntime "0"

command "sv_cvarempty"
//command "sv_cvar rate EQ 25000"
//command "sv_cvar cl_maxpackets EQ 100"
//command "sv_cvar snaps EQ 20"
//command "sv_cvar com_maxfps IN 40 125"
}
